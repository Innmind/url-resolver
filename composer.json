{
    "name": "innmind/url-resolver",
    "type": "library",
    "description": "Library to resolve relative urls to absolute ones",
    "keywords": ["url", "uri", "resolver"],
    "homepage": "http://github.com/Innmind/url-resolver",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/url-resolver/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/immutable": "~4.0|~5.0",
        "innmind/url": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\UrlResolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\UrlResolver\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.22",
        "innmind/coding-standard": "~2.0"
    }
}
